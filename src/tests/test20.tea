// 50_short_circuit

fn getint()->int;
fn putint(num:int);

let g:int = 0;

fn func(n:int)->int {
  g = g + n;
  putint(g);
  ret g;
}

fn main()->int {
  let i:int;
  i = getint();
  if ((i > 10) && (func(i) != 0)) {
    i = 1;
  } else {
    i = 0;
  }
  i = getint();
  if ((i > 11) && (func(i) != 0)) {
    i = 1;
  } else {
    i = 0;
  }
  i = getint();
  if ((i <= 99) || (func(i) != 0)) {
    i = 1;
  } else {
    i = 0;
  }
  i = getint();
  if ((i <= 100) || (func(i) != 0)) {
    i = 1;
  } else {
    i = 0;
  }
  if (!(func(99) != 0) && (func(100) != 0)) {
    i = 1;
  } else {
    i = 0;
  }
  ret 0;
}
