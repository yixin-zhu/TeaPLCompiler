struct MyStruct { 
    node:int,
    len:int  
}

let a:MyStruct;
let b:int;
let c:int;

fn foo(a:int, b:int)->int{
    if(a > b || b < a){
        ret b - 1;
    }else{
        ret b + 1;
    }
}

fn main(a2:int, b3:int)->int{
    a.node = 3;
    a.len = 20;

    while(a.len > 0){
        a.node = foo(a.node, a2);
        a.len = a.len - b3;
    }
    
    ret foo(a.node, a.len);
}


